// Write a simple loop that goes through each number in the array
# include <iostream> 
#include <vector> 
using namespace std;

vector<int> twoSum(vector<int>& nums, int target){
    for (int i = 0; i < nums.size(); i++){
        cout << "Checking number at index " << i << ": " << nums[i] << endl;
    }
    return {}; // empty return for now
}

int main() {
    vector<int> nums = {2, 7, 11, 15};
    int target = 9;
    
    twoSum(nums, target);
    
    return 0;
}
